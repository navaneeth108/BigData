

51,

SELECT
    name, population, area
FROM
    world
WHERE
    area >= 3000000 OR population >= 25000000
;


52,

SELECT 
name
from Customers 
where referee_id <> 2


53,

elect c.name as Customers from 
customers as c 
left join orders as o 
on c.id =  o.customerid
where o.id is null and o.customerid is null
